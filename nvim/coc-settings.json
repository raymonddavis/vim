{
    "coc.preferences.jumpCommand": "vsplit",
    "coc.preferences.formatOnType": true,
    "coc.preferences.formatOnSaveFiletypes": [
        "typescript",
        "typescriptreact",
        "javascript",
        "javascriptreact",
        "json",
        "graphql",
        "css",
        "markdown"
